{
  "expo": {
    "name": "Parasitour",
    "slug": "Parasitour",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logotipo.png",
      "resizeMode": "contain",
      "backgroundColor": "#4f40b5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#4f40b5"
      },
      "config": {
        "googleMaps": { "apiKey": "API_KEY" }
      },
      "package": "com.uname.parasitour"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
